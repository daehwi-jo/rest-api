                      //for IE Browsers
                    	if(navigator.userAgent.indexOf("MSIE 8.0") > -1 || navigator.userAgent.indexOf("MSIE 7.0") > -1|| document.documentMode == '7' ||  document.documentMode == '8') {
                    		ieLoadBugFix(script, function(){
