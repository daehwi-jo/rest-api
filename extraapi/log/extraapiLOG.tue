			}else if(gopaymethod == "onlyhpp" && payViewType == "popup" && acceptmethod.indexOf("onlyhpp_popup") > -1){ //휴대폰 다이렉트 결제인데 popup으로 진행하는 가맹점 덕분에 추가
				//팝업상태이면 오버레이로 변경 처리(Direct옵션일경우)
				INIStdPay.boolViewParentWindow = false
